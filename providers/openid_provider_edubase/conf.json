{
	"name": "edubase-de",
	"desc": "OpenID integration test",
	"url": "https://edubase-de.test-weedu.ch/sts/openid/connect",
	"oauth2": {
		"authorize": {
			"url": "https://edubase-de.test-weedu.ch/sts/openid/connect/authorize",
			"query": {
				"client_id": "{client_id}",
				"response_type": "code",
				"scope": "openid email",
				"redirect_uri": "{{callback}}",
				"state": "{{state}}"
			}
		},
		"access_token": {
			"url": "https://edubase-de.test-weedu.ch/sts/openid/connect/token",
			"extra": ["id_token"]
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": {
				"values": {
					"openid": "openid",
					"roles": "roles",
					"profile": "profile",
					"email": "email",
					"address": "address",
					"phone": "phone"
				}
			}
		}
	},
	"href": {
		"keys": "https://edubase-de.test-weedu.ch",
		"docs": "https://edubase-de.test-weedu.ch",
		"apps": "https://edubase-de.test-weedu.ch",
		"provider": "https://edubase-de.test-weedu.ch"
	}
}